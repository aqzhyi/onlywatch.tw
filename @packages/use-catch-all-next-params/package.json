{
  "private": false,
  "name": "@onlywatch/use-catch-all-next-params",
  "description": "A React Hook for managing NextParams of the route with Next.js Catch-All Segments",
  "version": "0.2.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "tsc": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "del dist coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@testing-library/react": "16.3.0",
    "@types/react": "19.2.0",
    "@vitest/coverage-v8": "3.2.4",
    "del": "8.0.1",
    "jsdom": "27.0.0",
    "next": "15.5.4",
    "prettier": "3.6.2",
    "prettier-plugin-jsdoc": "1.3.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tsdown": "0.15.6",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0"
  },
  "keywords": [
    "app-router",
    "catch-all",
    "dynamic-routes",
    "hook",
    "next",
    "nextjs",
    "params",
    "react",
    "segments"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aqzhyi/onlywatch.tw",
    "directory": "@packages/use-catch-all-next-params"
  }
}
