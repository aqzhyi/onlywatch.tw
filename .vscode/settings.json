{
  "biome.enabled": false,
  "cSpell.enabled": true,
  "cSpell.userWords": [
    // npm:chinese-conv
    "chinese-conv"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.enable": false,
  "oxc.enable": true,
  "prettier.enable": true,
  "tailwind-fold.foldStyle": "QUOTES",
  "tailwindCSS.classFunctions": ["cva", "cx", "clsx", "cn"],
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "classNames",
    "ngClass",
    "class:list"
  ],
  "typescript.tsdk": "node_modules/typescript/lib"
}
